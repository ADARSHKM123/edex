{
  "name": "node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server",
    "dev": "nodemon ./server",
    "start:prod": "set NODE_ENV=production && nodemon server.js",
    "watch:js": "parcel watch ./public/javascripts/index.js --out-dir ./public/javascripts/ --out-file bundle.js",
    "build": "parcel build ./public/javascripts/index.js --out-dir ./public/javascripts/ --out-file bundle.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@handlebars/allow-prototype-access": "^1.0.5",
    "a": "^2.1.2",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-handlebars": "^6.0.3",
    "express-rate-limit": "^6.3.0",
    "handlebars": "^4.7.7",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "rate-limit": "^0.1.1",
    "slugify": "^1.6.5",
    "stripe": "^7.0.0",
    "validator": "^13.7.0"
